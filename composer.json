{
    "name" : "sleepness/uber-translation-bundle",
    "type" : "symfony-bundle",
    "description" : "Extends Symfony2 Translation Component by storing all your translations in fast Memcached",
    "keywords" : [
        "translation",
        "i18n",
        "Symfony2",
        "memcached",
        "multilanguage",
        "locale"
    ],
    "homepage": "http://sleepness.github.io/",
    "license": "MIT",
    "authors" : [
        {
            "name" : "Oleg Zinchenko"
        },
        {
            "name" : "Viktor Novikov"
        },
        {
            "name" : "Alexandr Zhulev"
        }
    ],
    "require" : {
        "php": ">=5.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/twig-bundle": "~2.3"
    },
    "require-dev": {
        "symfony/finder": "~2.6",
        "symfony/console": "~2.6",
        "phpunit/phpunit": "~4.5"
    },
    "autoload" : {
        "psr-4" : { "Sleepness\\UberTranslationBundle\\" : "" }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
