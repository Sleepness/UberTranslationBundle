{% trans_default_domain 'SleepnessUberTranslationBundle' %}
<!DOCTYPE html>
<html>
    <head>
        <title>UberTranslationBundle</title>
        <meta charset="UTF-8"/>
        <link rel="shortcut icon" href="{{ asset('bundles/sleepnessubertranslation/images/favicon.ico') }}" type="image/x-icon">
        <link rel="icon" href="{{ asset('bundles/sleepnessubertranslation/images/favicon.ico') }}" type="image/x-icon">
        <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css">
        <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap-theme.min.css">
        <link rel="stylesheet" href="{{ asset('bundles/sleepnessubertranslation/css/styles.css') }}">
    </head>
    <body>
        <nav class="navbar navbar-inverse">
            <div class="container">
                <div class="navbar-header">
                    <a class="navbar-brand" href="{{ path('sleepness_translation_dashboard') }}">{{ 'logo_text'|trans }}</a>
                </div>
            </div>
        </nav>
        <div class="container">
            {% block uber_translation_bundle_content %}
            {% endblock %}
        </div>
        <script src="//code.jquery.com/jquery.js"></script>
        <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js"></script>
        <script src="{{ asset('bundles/sleepnessubertranslation/js/jquery.ubertranslation.js') }}"></script>
