<div class="panel panel-default">
    <!-- Default panel contents -->
    <div class="panel-heading">Panel heading</div>

    <!-- Table -->
    <table class="table">
        <thead>
        <tr>
            <th>Domain</th>
            <th>Key</th>
            <th>Messages</th>
        </tr>
        </thead>
        <tbody>
        {% for message in messages %}
            <tr>
                <td>{{ message.domain }}</td>
                <td>{{ message.key }}</td>
                <td>
                    {{ message.message.messageText }}
                    {{ message.message.locale }}
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

</div>
