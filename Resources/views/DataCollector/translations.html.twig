{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {# This toolbar item may appear along the top or bottom of the screen.#}
    {% set icon %}
    <span class="icon">
        <img src="data:image/png;base64,"/>
    </span>
    <span class="sf-toolbar-status">Locales: {{ collector.getAvailableLocales }}</span>
    {% endset %}

    {# Set the "link" value to false if you do not have a big "panel" section that you want to direct the user to. #}
    {{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { 'link': true }) }}
{% endblock %}

{% block head %}
    {{ parent() }} {# Use parent() to keep the default styles #}
{% endblock %}

{% block menu %}
    {# This left-hand menu appears when using the full-screen profiler. #}
    <span class="label">
        <span class="icon">
            <img src="data:image/png;base64,"/>
        </span>
        <strong>Translations</strong>
    </span>
{% endblock %}

{% block panel %}
    <h2>Translations (Memcache)</h2>

    {% for locale, translations in collector.getTranslations %}
        <hr/>
        <div>
            <h3>Locale: {{ locale }}</h3>

            {% if translations|length < 1 %}
                <div>
                    <i>No translations</i>
                </div>
            {% else %}
                {% for domain, messages in translations %}
                    <div>
                        <h4>Domain: {{ domain }}</h4>
                        <table>
                            <thead>
                            <tr>
                                <th>Key</th>
                                <th>Value</th>
                            </tr>
                            </thead>
                            <tbody>
                            {% for key, value in messages %}
                                <tr>
                                    <td>{{ key }}</td>
                                    <td>{{ value }}</td>
                                </tr>
                            {% endfor %}
                            </tbody>
                        </table>
                    </div>
                {% endfor %}
            {% endif %}
        </div>
    {% endfor %}

{% endblock %}
